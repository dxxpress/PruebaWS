<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/historialContenedor"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".tramosFragment"
    tools:layout_editor_absoluteY="25dp">

    <TextView
        android:id="@+id/llegada_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="136dp"
        android:layout_marginTop="40dp"
        android:gravity="center"
        android:text="Llegadatxt"
        android:textColor="@color/azul_chingon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/destino_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="136dp"
        android:layout_marginTop="64dp"
        android:gravity="center"
        android:text="Nombretxt"
        android:textColor="@color/azul_chingon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/folio_txt"
        android:layout_width="52dp"
        android:layout_height="16dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Foliotxt"
        android:textColor="@color/azul_chingon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_folio"
        android:layout_width="52dp"
        android:layout_height="16dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="16dp"
        android:text="Folio"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_llegada"
        android:layout_width="52dp"
        android:layout_height="18dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="40dp"
        android:text="Llegada"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_nombre"
        android:layout_width="52dp"
        android:layout_height="16dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="64dp"
        android:text="Destino"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="38dp"
        android:layout_height="34dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/logodx2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</android.support.constraint.ConstraintLayout>